//Clean Bandages no item type necessary
// Sir Parish Schattenwelt Germany
BUYVALUE=3 5
SELLVALUE=1 2
MATERIALS=1 cloth
WEIGHT=.1
DUPELIST=0ee9
DEF=BANDAGE
ONTRIGGER=TARGON_CHAR
	IF (<SRC.TARG.HITPOINTS>>=<SRC.TARG.STR> && (!(<SRC.TARG.FLAGS>&080)))
		SRC.SYSMESSAGE Do you wanna make a mummy?
		RETURN 1
	ENDIF
	IF (<SRC.FINDLAYER(1).ID>>0)
		SRC.FINDLAYER(1).UNEQUIP
	ENDIF
	IF (<SRC.FINDLAYER(2).ID>>0)
		SRC.FINDLAYER(2).UNEQUIP
	ENDIF
	IF (<SRC.TARG.FLAGS>&080) && (<SRC.HEALING>>=85.0)
		SRC.SYSMESSAGE You neutralize the poison!
		SRC.TARG.FLAGS=<SRC.TARG.FLAGS>&0ffffff7f
		SRC.TARG.UPDATE
		SRC.CONSUME 1 0e21
		SRC.NEWITEM=0e20
		SRC.ACT.ATTR=0
		SRC.ACT.TIMER=-1
		SRC.ACT.BOUNCE
		RETURN 1
	ELSEIF (<SRC.TARG.HITPOINTS>>=<SRC.TARG.STR>)
		SRC.SYSMESSAGE You fail to neutralize the poison!
		RETURN 1
	ENDIF
	// Just normal healing.
	SRC.SYSMESSAGE You put the clean bandages on the wounded <SRC.TARG.NAME>.
	RETURN 0

ONTRIGGER=TARGON_ITEM
	IF !(<SRC.TARG.ID>==02006)
		SRC.SYSMESSAGE You can't use this on a <SRC.TARG.NAME>! 
		RETURN 1
	ELSEIF (<SRC.TARG.DISTANCE>>2)
		SRC.SYSMESSAGE You are a healer not a medicineman, come closer!
		RETURN 1
	ELSEIF (<SRC.HEALING><95.0)
		SRC.SYSMESSAGE This is beyond your ability!
		RETURN 1
	ENDIF
	//free hands
	IF (<SRC.FINDLAYER(1).ID>>0)
		SRC.FINDLAYER(1).UNEQUIP
	ENDIF
	IF (<SRC.FINDLAYER(2).ID>>0)
		SRC.FINDLAYER(2).UNEQUIP
	ENDIF
	// Targ = a corpse?
	IF (<SRC.HEALING>>=95.0) && (<SRC.TARG.LINK.FLAGS>&02)
		// Targ = the ghost is near by ?
		IF (<SRC.TARG.LINK.DISTANCE>>8)
			SRC.SYSMESSAGE There is no ghost here!
			RETURN 1
		ENDIF
		IF (<SRC.RESTEST 20 0e21>)
			SRC.CONSUME 20 0e21
			SRC.EMOTE ressurect the <SRC.TARG.NAME>
			SRC.TARG.LINK.RESURRECT
			IF (<SRC.TARG.LINK.RESTEST 1 0204e>)  //remove death shroud, if there
				SRC.TARG.LINK.CONSUME 1 0204e
			ENDIF
		ELSE
			SRC.SYSMESSAGE You need more bandages!
		ENDIF
	ENDIF
	RETURN 1

CATEGORY=Items by Professions
SUBSECTION=Healer
DESCRIPTION=Clean Bandages

