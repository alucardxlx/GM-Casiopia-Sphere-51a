[ITEMDEF i_event_exitgate_blue]
ID=i_moongate_black
TYPE=t_script

ON=@DCLICK
  IF <SRC.ISGM>=1
    DIALOG d_event_exitgate
  ENDIF

ON=@STEP
  IF <SRC.TAG.Team>=1
    SRC.KARMA=<EVAL <SRC.TAG.Karma>> 
    SRC.FAME=<EVAL <SRC.TAG.Fame>> 
    SRC.KILLS=<EVAL <SRC.TAG.Kills>> 
    SRC.TAG.Team=
    SRC.TAG.Karma=
    SRC.TAG.Kills=
    SRC.TAG.Fame=
    SRC.UPDATE
    SRC.SPELLEFFECT=s_nightsight,1
    SRC.SPELLEFFECT=s_mana,100
    SRC.SPELLEFFECT=s_greater_heal,100
    SRC.SYSMESSAGE=Thank you for participating, your original Fame/Karma/Kills have been restored.
    IF <TAG.Win>=1
      SRC.SYSMESSAGE Congradulations on your win, you will now be sent to an appropriate town. Thanks for participating.
    ELSE
      SRC.SYSMESSAGE Sorry your team did not win, you will now be sent to an appropriate town. Thanks for participating.
    ENDIF
    IF <TAG.Prize>=1
      GIVEPOINTS <EVAL <MORE>>
      GIVETICKETS <EVAL <MORE2>>
    ENDIF
    IF (<MOREX>=0) & (<MOREY>=0) & (<MOREZ>=0) // Gate not marked
      IF <SRC.KILLS>>=5
        SRC.GO=buccaneer's den
      ELSE
        SRC.GO=britain
      ENDIF
    ELSE // Gate is marked with custom exit location
      SRC.GO=<MOREP>
    ENDIF
  ELSEIF <SRC.ISGM>=1
    IF (<MOREX>=0) & (<MOREY>=0) & (<MOREZ>=0) // Gate not marked
      IF <SRC.KILLS>>=5
        SRC.GO=buccaneer's den
      ELSE
        SRC.GO=britain
      ENDIF
    ELSE // Gate is marked with custom exit location
      SRC.GO=<MOREP>
    ENDIF
  ELSE
    SAY=<SRC.NAME> is trying to exit, but is leaving with the wrong team.
    SRC.MOVE=0,2,0
  ENDIF

