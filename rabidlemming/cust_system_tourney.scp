[FUNCTION f_event_bandages] // Arg = Class
IF (<EVAL <ARGS>> = 1) // Warrior
  VAR.Bandages=<EVAL (<TAG.MaxBandages>*30)>
ELSEIF (<EVAL <ARGS>> = 2) // Archer
  VAR.Bandages=<EVAL <TAG.MaxBandages>*20>
ELSEIF (<EVAL <ARGS>> = 3) // Mage
  VAR.Bandages=<EVAL <TAG.MaxBandages>*10>
ELSEIF (<EVAL <ARGS>> = 4) // Paladin
  VAR.Bandages=<TAG.MaxBandages>*40
ELSEIF (<EVAL <ARGS>> = 5) // Unused
ELSEIF (<EVAL <ARGS>> = 6) // Unused
ELSEIF (<EVAL <ARGS>> = 7) // Unused
ELSEIF (<EVAL <ARGS>> = 8) // Unused
ELSE
  RETURN=1
ENDIF
