// **************************************************
// Types
// **************************************************

[TYPEDEF t_event_ctf_cap_tile]
ON=@STEP
  IF (<SRC.TAG.Team> = <MORE>) // They are on this team
    IF <SRC.FINDLAYER(layer_hand1).ID=i_event_flag> // If they are on cap point with a flag
      IF (<SRC.FINDLAYER(layer_hand1).MORE> != <EVAL SRC.TAG.Team>) // Make sure some idiot isnt capturing his own flag
        IF (<LINK> = 04fffffff)
          TRY SRC.UID.<SRC.FINLAYER(layer_hand1).MORE2>.MORE2=1 // Change status of other team's caputre tile to captured.
          SRC.FINDLAYER(layer_hand1).REMOVE
        ELSE
          SRC.SYSMESSAGE You must return your own flag before you can caputre.
        ENDIF
      ELSE // Kill a person capturing their own flag
        SRC.KILL
      ENDIF
     ELSE // Prevent sitting on the flag
      RETURN=1
     ENDIF
   ENDIF

ON=@DCLICK
  IF <SRC.GM>=1
    IF <LINK>=04fffffff
      SRC.NEWITEM=i_event_flag
      SRC.ACT.P=<P>
      SRC.ACT.NUDGEUP=6
      SRC.ACT.MORE2=<UID>
      LINK=<SRC.ACT.UID>
      SRC.SYSMESSAGE=<NAME> Initialized
    ELSE
      SRC.SYSMESSAGE <NAME> has already been intitialized, captured and dropped flags will respawn automaticaly.
    ENDIF
  ENDIF

ON=@TIMER
  IF (<LINK.LINK.HITS>=0) || (<MORE2>=1) // If flag holder died or flag was captured
    LINK.NEWITEM=i_event_flag
    IF (<MORE> = 1)
      LINK.ACT.NAME=Blue Team Flag
      LINK.ACT.COLOR=color_team_blue
    ELSEIF (<MORE> = 2)
      LINK.ACT.NAME=Red Team Flag
      LINK.ACT.COLOR=color_team_red
    ELSEIF (<MORE> = 3)
      LINK.ACT.NAME=White Team Flag
      LINK.ACT.COLOR=color_team_white
    ELSEIF (<MORE> = 4)
      LINK.ACT.NAME=Green Team Flag
      LINK.ACT.COLOR=color_team_green
    ELSEIF (<MORE> = 5)
      LINK.ACT.NAME=Black Team Flag
      LINK.ACT.COLOR=color_team_black
    ELSEIF (<MORE> = 6)
      LINK.ACT.NAME=Orange Team Flag
      LINK.ACT.COLOR=color_team_orange
    ELSEIF (<MORE> = 7)
      LINK.ACT.NAME=Purple Team Flag
      LINK.ACT.COLOR=color_team_purple
    ELSEIF (<MORE> = 8)
      LINK.ACT.NAME=Yellow Team Flag
      LINK.ACT.COLOR=color_team_yellow
    ENDIF
    LINK.ACT.P=<P>
    LINK.ACT.NUDGEUP=6
    LINK.ACT.MORE=<MORE>
    LINK.ACT.MORE2=<UID>
    LINK.REMOVE
    LINK=<SRC.ACT.UID>
  ENDIF
  TIMER=5
  RETURN=1

