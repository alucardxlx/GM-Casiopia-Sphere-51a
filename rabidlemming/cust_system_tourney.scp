[ITEMDEF i_event_gate_blue]
NAME=Blue Team Moongate
ID=i_moongate_black
TYPE=t_script

ON=@DCLICK
  IF (<SRC.ISGM> = 1)
    DIALOG=d_event_gate
  ELSE
    RETURN=1
  ENDIF

ON=@STEP
  VAR.WEIGHT=6.0 // Used as default weight of the player (note, you will have to increase this to allow in mounts)
  IF (<MOREX> = 0) || (<MOREY> = 0) || (<MORE> = 0) || (<MORE2> = 0) // Assure the gate has a location, spellbook and team
    SAY=This gate has not been set up yet, please wait for a GM to do so.
    RETURN=1
  ELSE
    // Start check of banlist for player's account, if found they will be moved aside.
    IF (<SRC.ISGM> = 0)
      IF (<SRC.FINDLAYER(layer_ring).ID> > 0) // Check for rings for married couples and adjust weight
        VAR.WEIGHT=<VAR.WEIGHT>+<SRC.FINDLAYER(layer_ring).WEIGHT>
      ENDIF
      IF (<SRC.FINDLAYER(layer_ears).ID> > 0) // Put any earings in bank
        VAR.WEIGHT=<VAR.WEIGHT>+<SRC.FINDLAYER(layer_ear).WEIGHT>
      ENDIF
      IF <SRC.WEIGHT> <= <VAR.WEIGHT>
        IF <SRC.RESTEST=1 i_spellbook> // Spellbook check since they are weightless(in 51a at least)
          SAY=<SRC.NAME> is trying to sneak items in!
          SRC.MOVE=2
          SRC.SYSMESSAGE=You have been sent away for trying to sneak items in.
        ELSE
          IF (<SRC.TAG.Team> = 0) // Not yet a team member, store F/K/K and equip.
            SOUND=00216
            SRC.ANIM=17
            SRC.TAG.Fame=<SRC.Fame> // Backup Fame
            SRC.TAG.Karma=<SRC.Karma> // Backup Karma
            SRC.TAG.Kills=<SRC.Kills> // Backup Kills
            SRC.TAG.Team=<EVAL <MORE2>> // Set team on person
            SRC.SPELLEFFECT=s_night_sight,1
            SRC.SPELLEFFECT=s_mana,100
            SRC.SPELLEFFECT=s_greater_heal,100
            SAY <SRC.NAME> has joined the team.
            SRC.SYSMESSAGE=Thank you <SRC.NAME>, your Fame/Karma/Kills have been memorized.
          ELSEIF (<SRC.TAG.Team> = <MORE2>) // Already a member of this team, re-equip player.
            SRC.SYSMESSAGE=You are being re-equiped, Fame/Karma/Kills were not memorized this time.
          ELSE
            SRC.SYSMESSAGE=You are not a member of this team.
            RETURN=1
          ENDIF
          IF (<MORE2> = 1) // Blue
            SRC.NEWITEM=i_event_blue_coin
          ELSEIF (<MORE2> = 2) // Red
            SRC.NEWITEM=i_event_red_coin
          ELSEIF (<MORE2> = 3) // White
            SRC.NEWITEM=i_event_white_coin
          ELSEIF (<MORE2> = 4) // Green
            SRC.NEWITEM=i_event_green_coin
          ELSEIF (<MORE2> = 5) // Black
            SRC.NEWITEM=i_event_black_coin
          ELSEIF (<MORE2> = 6) // Orange
            SRC.NEWITEM=i_event_orange_coin
          ELSEIF (<MORE2> = 7) // Purple
            SRC.NEWITEM=i_event_purple_coin
          ELSEIF (<MORE2> = 8) // Yellow
            SRC.NEWITEM=i_event_yellow_coin
          ENDIF
          SRC.ACT.MORE2=<MORE2>
          SRC.ACT.TAG.MaxManaPots=<TAG.MaxManaPots>
          SRC.ACT.TAG.TAG.MaxGHealPots=<TAG.MaxGHealPots>
          SRC.ACT.TAG.TAG.MaxBandages=<TAG.MaxBandages>
          SRC.GO=<MOREP> // Transport to tourney area marked or specified in gump
        ENDIF
      ELSE
        SAY=<SRC.NAME> is trying to sneak items in!
        SRC.MOVE=2
      ENDIF
    ELSE // They are a gm, let them right in
      SRC.GO=<MOREP>
    ENDIF
  ENDIF

