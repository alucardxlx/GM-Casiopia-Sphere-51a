[ITEMDEF i_roulette_wheel]
ID=i_shield_round_metal
NAME=roulette wheel
TYPE=t_script

ON=@DCLICK
  IF <DISTANCE> > 1
    SRC.SYSMESSAGE You are too far away to use this.
    RETURN=1
  ENDIF
  SRC.TAG.GamblingDevice=<UID>
  SRC.ITEMMENU menu_roulette
  RETURN=1

ON=@TIMER
  IF !(<LINK>=04fffffff)
    IF (<LINK.TYPE>=t_casino_sign)
      LINK.MORE=<LINK.MORE>+<MORE>
      LINK.MORE2=<LINK.MORE2>+<MORE2>
      MORE=0
      MORE2=0
    ENDIF
  ENDIF
  TIMER=600 // Update casino sign with wins/losses every 10 minutes
  RETURN=1

ON=@Create
  COLOR=079c
  ATTR=attr_move_never
  TIMER=600

CATEGORY=Custom Items
SUBSECTION=Casinos & Gambling
DESCRIPTION=Roulette Wheel

[ITEMDEF i_slot_machine]
ID=00edb
NAME=slot machine
TYPE=t_script
FLIP=1

ON=@DCLICK
  IF <DISTANCE> > 1
    SRC.SYSMESSAGE You are too far away to use this.
    RETURN=1
  ENDIF
  IF <SRC.RESTEST=<MOREX> i_gold>
    SRC.CONSUME=<MOREX> i_gold
    SOUND=0032
    IF !(<NAME>=slot machine - <EVAL <MOREX>> gold)
      NAME=slot machine - <EVAL <MOREX>> gold
    ENDIF
    SRC.TAG.GamblingDevice=<UID>
    SRC.SYSMESSAGE You put <EVAL <MOREX>> gold in the <NAME>.
    TRY SRC.UID.<SRC.TAG.GamblingDevice>.SFX=0056
    VAR.Number_1=<EVAL RAND(8)>
    VAR.Number_2=<EVAL RAND(8)>
    VAR.Number_3=<EVAL RAND(8)>
    IF (<VAR.Number_1>=0)
      SRC.SYSMESSAGE Lemmon
    ELSEIF (<VAR.Number_1>=01)
      SRC.SYSMESSAGE Orange
    ELSEIF (<VAR.Number_1>=02)
      SRC.SYSMESSAGE Cherry
    ELSEIF (<VAR.Number_1>=03)
      SRC.SYSMESSAGE Dagger
    ELSEIF (<VAR.Number_1>=04)
      SRC.SYSMESSAGE Axe
    ELSEIF (<VAR.Number_1>=05)
      SRC.SYSMESSAGE Mace
    ELSEIF (<VAR.Number_1>=06)
      SRC.SYSMESSAGE Sword
    ELSEIF (<VAR.Number_1>=07)
      SRC.SYSMESSAGE Chaos Symbol
    ELSEIF (<VAR.Number_1>=08)
      SRC.SYSMESSAGE Order Symbol
    ENDIF
    IF (<VAR.Number_2>=0)
      SRC.SYSMESSAGE Lemmon
    ELSEIF (<VAR.Number_2>=01)
      SRC.SYSMESSAGE Orange
    ELSEIF (<VAR.Number_2>=02)
      SRC.SYSMESSAGE Cherry
    ELSEIF (<VAR.Number_2>=03)
      SRC.SYSMESSAGE Dagger
    ELSEIF (<VAR.Number_2>=04)
      SRC.SYSMESSAGE Axe
    ELSEIF (<VAR.Number_2>=05)
      SRC.SYSMESSAGE Mace
    ELSEIF (<VAR.Number_2>=06)
      SRC.SYSMESSAGE Sword
    ELSEIF (<VAR.Number_2>=07)
      SRC.SYSMESSAGE Chaos Symbol
    ELSEIF (<VAR.Number_2>=08)
      SRC.SYSMESSAGE Order Symbol
    ENDIF
    IF (<VAR.Number_3>=0)
      SRC.SYSMESSAGE Lemmon
    ELSEIF (<VAR.Number_3>=01)
      SRC.SYSMESSAGE Orange
    ELSEIF (<VAR.Number_3>=02)
      SRC.SYSMESSAGE Cherry
    ELSEIF (<VAR.Number_3>=03)
      SRC.SYSMESSAGE Dagger
    ELSEIF (<VAR.Number_3>=04)
      SRC.SYSMESSAGE Axe
    ELSEIF (<VAR.Number_3>=05)
      SRC.SYSMESSAGE Mace
    ELSEIF (<VAR.Number_3>=06)
      SRC.SYSMESSAGE Sword
    ELSEIF (<VAR.Number_3>=07)
      SRC.SYSMESSAGE Chaos Symbol
    ELSEIF (<VAR.Number_3>=08)
      SRC.SYSMESSAGE Order Symbol
    ENDIF
  
    // Payoff section: if all 3 symbols came up same, pay...
    // <what they put in> * <jackpot level multiplier> * <slot cost prize increaser>
    IF (<VAR.Number_1>=0) & (<VAR.Number_2>=0) & (<VAR.Number_3>=0)
      SRC.NEWITEM=i_gold
      SRC.ACT.AMOUNT=<EVAL (<MOREX>*2*(<MOREX>/10>))>
      SRC.ACT.BOUNCE
      SRC.EMOTE win a <EVAL (<MOREX>*2*<MOREX>/10))> gold jackpot
      TRY SRC.UID.<SRC.TAG.GamblingDevice>.MORE=<SRC.UID.<SRC.TAG.GamblingDevice>.MORE>+(<MOREX>*2*(<MOREX>/10))+-<MOREX> // Move win counter
    ELSEIF (<VAR.Number_1>=01) & (<VAR.Number_2>=01) & (<VAR.Number_3>=01)
      SRC.NEWITEM=i_gold
      SRC.ACT.AMOUNT=<EVAL (<MOREX>*5*(<MOREX>/10>))>
      SRC.ACT.BOUNCE
      SRC.EMOTE win a <EVAL (<MOREX>*5*(<MOREX>/10))> gold jackpot
      TRY SRC.UID.<SRC.TAG.GamblingDevice>.MORE=<SRC.UID.<SRC.TAG.GamblingDevice>.MORE>+(<MOREX>*5*(<MOREX>/10))+-<MOREX> // Move win counter
    ELSEIF (<VAR.Number_1>=02) & (<VAR.Number_2>=02) & (<VAR.Number_3>=02)
      SRC.NEWITEM=i_gold
      SRC.ACT.AMOUNT=<EVAL (<MOREX>*10*(<MOREX>/10>))>
      SRC.ACT.BOUNCE
      SRC.EMOTE win a <EVAL (<MOREX>*10*(<MOREX>/10))> gold jackpot
      TRY SRC.UID.<SRC.TAG.GamblingDevice>.MORE=<SRC.UID.<SRC.TAG.GamblingDevice>.MORE>+(<MOREX>*10*(<MOREX>/10))+-<MOREX> // Move win counter
    ELSEIF (<VAR.Number_1>=03) & (<VAR.Number_2>=03) & (<VAR.Number_3>=03)
      SRC.NEWITEM=i_gold
      SRC.ACT.AMOUNT=<EVAL (<MOREX>*20*(<MOREX>/10>))>
      SRC.ACT.BOUNCE
      SRC.EMOTE win a <EVAL (<MOREX>*20*(<MOREX>/10))> gold jackpot
      TRY SRC.UID.<SRC.TAG.GamblingDevice>.MORE=<SRC.UID.<SRC.TAG.GamblingDevice>.MORE>+(<MOREX>*20*(<MOREX>/10))+-<MOREX> // Move win counter
    ELSEIF (<VAR.Number_1>=04) & (<VAR.Number_2>=04) & (<VAR.Number_3>=04)
      SRC.NEWITEM=i_gold
      SRC.ACT.AMOUNT=<EVAL (<MOREX>*30*(<MOREX>/10>))>
      SRC.ACT.BOUNCE
      SRC.EMOTE win a <EVAL (<MOREX>*30*(<MOREX>/10))> gold jackpot
      TRY SRC.UID.<SRC.TAG.GamblingDevice>.MORE=<SRC.UID.<SRC.TAG.GamblingDevice>.MORE>+(<MOREX>*30*(<MOREX>/10))+-<MOREX> // Move win counter
    ELSEIF (<VAR.Number_1>=05) & (<VAR.Number_2>=05) & (<VAR.Number_3>=05)
      SRC.NEWITEM=i_gold
      SRC.ACT.AMOUNT=<EVAL (<MOREX>*45*(<MOREX>/10>))>
      SRC.ACT.BOUNCE
      SRC.EMOTE win a <EVAL (<MOREX>*45*(<MOREX>/10))> gold jackpot
      TRY SRC.UID.<SRC.TAG.GamblingDevice>.MORE=<SRC.UID.<SRC.TAG.GamblingDevice>.MORE>+(<MOREX>*45*(<MOREX>/10))+-<MOREX> // Move win counter
    ELSEIF (<VAR.Number_1>=06) & (<VAR.Number_2>=06) & (<VAR.Number_3>=06)
      SRC.NEWITEM=i_gold
      SRC.ACT.AMOUNT=<EVAL (<MOREX>*60*(<MOREX>/10>))>
      SRC.ACT.BOUNCE
      SRC.EMOTE win a <EVAL (<MOREX>*60*(<MOREX>/10))> gold jackpot
      TRY SRC.UID.<SRC.TAG.GamblingDevice>.MORE=<SRC.UID.<SRC.TAG.GamblingDevice>.MORE>+(<MOREX>*60*(<MOREX>/10))+-<MOREX> // Move win counter
    ELSEIF (<VAR.Number_1>=07) & (<VAR.Number_2>=07) & (<VAR.Number_3>=07)
      SRC.NEWITEM=i_gold
      SRC.ACT.AMOUNT=<EVAL (<MOREX>*80*(<MOREX>/10))>
      SRC.ACT.BOUNCE
      SRC.EMOTE win a <EVAL (<MOREX>*80*(<MOREX>/10))> gold jackpot
      TRY SRC.UID.<SRC.TAG.GamblingDevice>.MORE=<SRC.UID.<SRC.TAG.GamblingDevice>.MORE>+(<MOREX>*80*(<MOREX>/10))+-<MOREX> // Move win counter
    ELSEIF (<VAR.Number_1>=08) & (<VAR.Number_2>=08) & (<VAR.Number_3>=08)
      SRC.NEWITEM=i_gold
      SRC.ACT.AMOUNT=<EVAL (<MOREX>*100*(<MOREX>/10))>
      SRC.ACT.BOUNCE
      SRC.EMOTE win a <EVAL (<MOREX>*100*(<MOREX>/10))> gold jackpot
      TRY SRC.UID.<SRC.TAG.GamblingDevice>.MORE=<SRC.UID.<SRC.TAG.GamblingDevice>.MORE>+(<MOREX>*100*(<MOREX>/10))+-<MOREX> // Move win counter
    ELSE
      TRY SRC.UID.<SRC.TAG.GamblingDevice>.MORE2=<SRC.UID.<SRC.TAG.GamblingDevice>.MORE2>+<MOREX> // Move loss counter
    ENDIF
    SRC.TAG.GamblingDevice=
  ELSE
    DORAND 4
      SRC.SYSMESSAGE You are out of money, perhaps the you could pawn the clothes you are wearing.
      SRC.SYSMESSAGE It looks like you have gambled your money away already, come back with more.
      SRC.SYSMESSAGE Have you thought about selling your house? because your broke now.
