[MENU menu_roulette_confirmation]
Finished making bets?

ON=0 Done making bets
  VAR.NUMBER=(<EVAL RAND(27)>+1)
  IF (<VAR.NUMBER>=01) | (<VAR.NUMBER>=03) | (<VAR.NUMBER>=05) | (<VAR.NUMBER>=07) | (<VAR.NUMBER>=09) | (<VAR.NUMBER>=012) | (<VAR.NUMBER>=014) | (<VAR.NUMBER>=016) | (<VAR.NUMBER>=017) | (<VAR.NUMBER>=019) | (<VAR.NUMBER>=021) | (<VAR.NUMBER>=023) | (<VAR.NUMBER>=025) | (<VAR.NUMBER>=027) | (<VAR.NUMBER>=030) | (<VAR.NUMBER>=032) | (<VAR.NUMBER>=034) | (<VAR.NUMBER>=036)
    TRY SRC.UID.<TAG.GamblingDevice>.EMOTE=stop, the ball lands on red <EVAL <VAR.NUMBER>>
  ELSEIF (<VAR.NUMBER>=037) | (<VAR.NUMBER>=038)
    IF (<VAR.NUMBER>=037) // 37 is used as 00
      TRY SRC.UID.<TAG.GamblingDevice>.EMOTE=stop, the ball lands on green 0
    ELSE
      TRY SRC.UID.<TAG.GamblingDevice>.EMOTE=stop, the ball lands on green 00
    ENDIF
  ELSE
    TRY SRC.UID.<TAG.GamblingDevice>.EMOTE=stop, the ball lands on black <EVAL <VAR.NUMBER>>
  ENDIF
  IF (<TAG.Bet_1>=<VAR.NUMBER>) | (<TAG.Bet_2>=<VAR.NUMBER>) | (<TAG.Bet_3>=<VAR.NUMBER>) | (<TAG.Bet_4>=<VAR.NUMBER>) | (<TAG.Bet_5>=<VAR.NUMBER>) | (<TAG.Bet_6>=<VAR.NUMBER>) | (<TAG.Bet_7>=<VAR.NUMBER>) | (<TAG.Bet_8>=<VAR.NUMBER>) | (<TAG.Bet_9>=<VAR.NUMBER>) | (<TAG.Bet_10>=<VAR.NUMBER>) | (<TAG.Bet_11>=<VAR.NUMBER>) | (<TAG.Bet_12>=<VAR.NUMBER>)
    SRC.NEWITEM=i_gold
    SRC.ACT.AMOUNT=(<SRC.TAG.Bet>*35)
    SRC.ACT.BOUNCE
    SOUND=0032
    SRC.SYSMESSAGE You have won <EVAL <SRC.TAG.Bet>*35> gold.
    TRY SRC.UID.<TAG.GamblingDevice>.MORE=<SRC.UID.<TAG.GamblingDevice>.MORE>+((<SRC.TAG.Bet>*35)+-<SRC.TAG.Bet>) // Move win counter
  ELSE // Loosing number
    IF (<TAG.Bet_2>=0)
      VAR.LOSS=(<SRC.TAG.Bet>*1)
    ELSEIF (<TAG.Bet_3>=0)
      VAR.LOSS=(<SRC.TAG.Bet>*2)
    ELSEIF (<TAG.Bet_4>=0)
      VAR.LOSS=(<SRC.TAG.Bet>*3)
    ELSEIF (<TAG.Bet_5>=0)
      VAR.LOSS=(<SRC.TAG.Bet>*4)
    ELSEIF (<TAG.Bet_6>=0)
      VAR.LOSS=(<SRC.TAG.Bet>*5)
    ELSEIF (<TAG.Bet_7>=0)
      VAR.LOSS=(<SRC.TAG.Bet>*6)
    ELSEIF (<TAG.Bet_8>=0)
      VAR.LOSS=(<SRC.TAG.Bet>*7)
    ELSEIF (<TAG.Bet_9>=0)
      VAR.LOSS=(<SRC.TAG.Bet>*8)
    ELSEIF (<TAG.Bet_10>=0)
      VAR.LOSS=(<SRC.TAG.Bet>*9)
    ELSEIF (<TAG.Bet_11>=0)
      VAR.LOSS=(<SRC.TAG.Bet>*10)
    ELSEIF !(<TAG.Bet_12>=0)
      VAR.LOSS=(<SRC.TAG.Bet>*11)
    ELSE
      VAR.LOSS=(<SRC.TAG.Bet>*12)
    ENDIF
  ENDIF
  SRC.SYSMESSAGE You have lost <EVAL <VAR.LOSS>> gold.
  TRY SRC.UID.<TAG.GamblingDevice>.MORE2=<SRC.UID.<TAG.GamblingDevice>.MORE>+<VAR.LOSS> // Move loss counter
  // Clear tags on player
  TAG.GamblingDevice=
  TAG.Bet=
  TAG.Bet_1=
  TAG.Bet_2=
  TAG.Bet_3=
  TAG.Bet_4=
  TAG.Bet_5=
  TAG.Bet_6=
  TAG.Bet_7=
  TAG.Bet_8=
  TAG.Bet_9=
  TAG.Bet_10=
  TAG.Bet_11=
  TAG.Bet_12=

